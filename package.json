{
  "name": "vetted",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.16.1",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "prisma:generate": "pnpm --filter @vetted/infra-db db:generate && pnpm --filter @vetted/db build",
    "prisma:migrate": "pnpm --filter @vetted/infra-db db:migrate",
    "db:seed": "pnpm --filter @vetted/infra-db db:seed",
    "env:validate": "tsx scripts/validate-env.ts",
    "env:setup": "cp env.local.example .env.local",
    "clean:turbo": "pnpm dlx rimraf .turbo \"**/.turbo\"",
    "clean:modules": "pnpm dlx rimraf node_modules \"**/node_modules\" && pnpm store prune",
    "clean:outputs": "pnpm dlx rimraf \"**/.next\" \"**/dist\" \"**/build\" \"**/.cache\"",
    "rebuild:fresh": "pnpm run clean:turbo && pnpm run clean:modules && pnpm run clean:outputs && pnpm install && pnpm turbo run build"
  },
  "devDependencies": {
    "@types/node": "^20.19.14",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prisma": "^5.19.1",
    "tailwindcss": "^3.4.10",
    "dotenv": "^16.4.5",
    "tsx": "^4.7.0",
    "turbo": "^2.1.3",
    "typescript": "^5.6.2"
  }
}
