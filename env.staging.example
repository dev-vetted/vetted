# Staging Environment
# Copy this to .env.staging for staging deployments

# Environment Configuration
APP_ENV=staging
NODE_ENV=production

# API URLs
NEXT_PUBLIC_API_URL=https://staging-bff.vercel.app
EXPO_PUBLIC_API_URL=https://staging-bff.vercel.app

# Database
DATABASE_URL=postgresql://staging_user:staging_pass@staging-db.supabase.co:5432/postgres

# Payments (Test Mode for Staging)
PAYMENTS_MODE=test
RAZORPAY_KEY_ID=rzp_test_staging_key_here
RAZORPAY_KEY_SECRET=staging_secret_here
NEXT_PUBLIC_RAZORPAY_KEY_ID=rzp_test_staging_key_here
EXPO_PUBLIC_RAZORPAY_KEY_ID=rzp_test_staging_key_here

# Video/Communication
AGORA_APP_ID=staging_agora_app_id
AGORA_APP_CERT=staging_agora_cert
NEXT_PUBLIC_AGORA_APP_ID=staging_agora_app_id
EXPO_PUBLIC_AGORA_APP_ID=staging_agora_app_id

# Storage (AWS S3)
S3_BUCKET=vetted-staging-bucket
S3_REGION=us-east-1
S3_ACCESS_KEY_ID=staging_s3_access_key
S3_SECRET_ACCESS_KEY=staging_s3_secret

# Supabase
SUPABASE_URL=https://staging.supabase.co
SUPABASE_ANON_KEY=staging_anon_key
SUPABASE_SERVICE_KEY=staging_service_key
NEXT_PUBLIC_SUPABASE_URL=https://staging.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=staging_anon_key
EXPO_PUBLIC_SUPABASE_URL=https://staging.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=staging_anon_key

# Cache / Queue (Upstash Redis)
UPSTASH_REDIS_URL=redis://staging-redis.upstash.io:6379
UPSTASH_REDIS_TOKEN=staging_redis_token

# Image Processing
IMAGEKIT_PUBLIC_KEY=staging_imagekit_public_key
IMAGEKIT_PRIVATE_KEY=staging_imagekit_private_key
IMAGEKIT_URL_ENDPOINT=https://ik.imagekit.io/staging_id
